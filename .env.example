# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# API Configuration
# For local development: http://localhost:5001/dispersed/us-central1/api
# For production: https://us-central1-dispersed.cloudfunctions.net/api
REACT_APP_API_URL=https://us-central1-dispersed.cloudfunctions.net/api

# ArcGIS Configuration
# Using USFS Motor Vehicle Use Map (MVUM) service for forest roads and trails
# Map center defaults - adjust to your preferred location
REACT_APP_MAP_CENTER_LNG=-105.6598
REACT_APP_MAP_CENTER_LAT=39.821
REACT_APP_MAP_ZOOM=11
